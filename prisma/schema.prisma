generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model cat_usuarios {
  id_usuario     Int     @unique
  username       String  @unique
  password       String?
  nombre         String?
  ap_paterno     String?
  ap_materno     String?
  email          String  @unique
  perfil_usuario Int
  estatus        Int
}
